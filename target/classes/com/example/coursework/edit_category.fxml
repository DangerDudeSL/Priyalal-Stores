        </Pane>
        <TextField fx:id="editCategoryOldName" layoutX="66.0" layoutY="63.0" prefHeight="25.0" prefWidth="181.0" promptText="Find Category By Name" />
        <TextField fx:id="editCategoryNewName" layoutX="64.0" layoutY="107.0" prefHeight="25.0" prefWidth="181.0" promptText="Type the new Category Name" />
        <Button fx:id="editBtn" layoutX="231.0" layoutY="165.0" mnemonicParsing="false" onAction="#categoryEdit" text="Edit" />
        <Button fx:id="editCategoryCleanBtn" layoutX="30.0" layoutY="165.0" mnemonicParsing="false" onAction="#editCategoryClean" text="Clear" />
