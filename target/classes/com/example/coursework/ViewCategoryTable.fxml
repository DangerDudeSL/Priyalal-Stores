<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="332.0" prefWidth="498.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.ViewCategoryTableController">
    <children>
        <TableView fx:id="categoryTable" layoutX="-1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="247.0" prefWidth="498.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="298.0" text="Category Name" />
                <TableColumn fx:id="idColumn" prefWidth="184.0" text="Category id" />
            </columns>
        </TableView>
      <Button fx:id="addCategoryBtn" layoutX="7.0" layoutY="275.0" mnemonicParsing="false" onAction="#tableAddCategory" text="Add Category" />
      <Button fx:id="deleteCategoryTableBtn" layoutX="267.0" layoutY="275.0" mnemonicParsing="false" onAction="#DeleteCategoryTable" text="Delete Category" />
      <Button fx:id="editCategoryTableBtn" layoutX="141.0" layoutY="275.0" mnemonicParsing="false" onAction="#tableEditCategory" text="Edit Category" />
      <Button fx:id="refreshBtn" layoutX="396.0" layoutY="275.0" mnemonicParsing="false" onAction="#refreshCategoryTable" text="Refresh Table" />
    </children>
</AnchorPane>
